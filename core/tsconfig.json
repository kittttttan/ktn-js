{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "noImplicitAny": false,
        "rootDir": ".",
        "sourceMap": true
    },
    "exclude": [
        "node_modules",
        "doc",
        "typings"
    ],
    "typedocOptions": {
        "out": "doc",
        "mode": "file",
        "includeDeclarations": true,
        "hideGenerator": false
    }
}
