{
  "env": {
    "browser": true,
    "node": true,
    "amd": false
  },

  "rules": {
    "no-console": 0,
    "no-comma-dangle": 2,
    "no-debugger": 2,
    "no-empty": 2,
    "no-obj-calls": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "no-dupe-keys": 2,
    "no-empty-class": 2,
    "no-func-assign": 2,
    "no-control-regex": 1,

    "no-caller": 2,
    "curly": 2,
    "eqeqeq": 1,
    "dot-notation": 1,
    "no-catch-shadow": 2,
    "no-eval": 1,
    "no-with": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-floating-decimal": 0,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-native-reassign": 2,
    "no-delete-var": 2,
    "no-return-assign": 2,
    "no-label-var": 2,
    "wrap-iife": 1,
    "no-self-compare": 2,
    "no-eq-null": 2,
    "no-multi-str": 2,
    "no-loop-func": 2,
    "no-empty-label": 1,
    "unnecessary-strict": 1,
    "no-unused-expressions": 0,
    "no-unused-vars": 1,
    "no-script-url": 2,
    "no-proto": 2,
    "no-iterator": 2,
    "no-else-return": 1,
    "no-shadow": 2,
    "no-alert": 2,
    "no-use-before-define": 0,
    "no-redeclare": 2,
    "no-global-strict": 1,
    "strict": 1,
    "no-div-regex": 1,

    "camelcase": 0,
    "new-cap": 2,
    "quote-props": 0,
    "semi": 2,
    "no-ternary": 0,
    "consistent-this": [0, "self"],
    "one-var": 0,
    "no-mixed-requires": [0, false],
    "no-wrap-func": 2,
    "complexity": [1, 7],
    "new-parens": 1,
    "wrap-regex": 0,

    "no-plusplus": 0,
    "no-bitwise": 0,
    "guard-for-in": 0,
    "max-statements": [1, 30],
    "max-params": [1, 4],
    "max-depth": [0, 4],
    "max-len": [1, 80, 4],

    "no-ex-assign": 2,
    "no-implied-eval": 2,
    "no-fallthrough": 1,
    "no-new-wrappers": 2,
    "no-new-array": 2,
    "no-new-object": 2,
    "no-sync": 0,
    "no-underscore-dangle": 0,

    "brace-style": 0,
    "block-scoped-var": 0,
    "quotes": [1, "single"],
    "radix": 1,
    "regex-spaces": 1
  }
}
