    "name": "kittttttan",
    "url": "https://kittttttan.info/"
  },
  "homepage": "https://github.com/kittttttan/ktn-js",
  "repository": {
    "type": "git",
    "url": "git://github.com/kittttttan/ktn-js.git"
  },
  "private": true,
  "files": [
    "README.md",
    "package.json",
    "index.js",
    "index.map",
    "index.d.ts",
    "src/**/*.js",
    "src/**/*.map",
    "src/**/*.d.ts"
  ],
  "main": "src/primality.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "doc": "typedoc src/",
    "clean": "rm -f src/*.js src/*.map spec/*.js spec/*.map index.js index.js.map",
    "lint": "tslint -c ../tslint.json src/*.ts> lint.log",
    "pretest": "tsc",
    "test": "istanbul cover node_modules/jasmine/bin/jasmine.js",
    "posttest": "cd coverage && remap-istanbul -i coverage.json -o html-report -t html"
  },
  "dependencies": {
    "@ktn/type": "file:../type"
  },
  "devDependencies": {
    "@types/jasmine": "",
    "jasmine": "",
    "istanbul": "",
    "remap-istanbul": "",
    "typescript": ""
  }
}
